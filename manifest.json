{
  "description": "..... NextJS Utils Extension .......",
  "manifest_version": 2,
  "name": "NextJS Utilities Extension",
  "version": "1.0",
  "homepage_url": "https://github.com/",
  "icons": {
    "32": "icons/icon-N-32x32.png"
  },
  "permissions": ["activeTab", "contextMenus"],
  "background": {
    "scripts": ["background.js", "lib/jquery.min.js", "nextutils.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_idle",
      "js": [
        "lib/jquery.min.js",
        "content_scripts/nextjsonload.js",
        "content_scripts/content-script.js"
      ],
      "css": ["BodyEnhance.css"]
    }
  ],
  "web_accessible_resources": ["*"],
  "browser_action": {
    "default_icon": "icons/icon-N-32x32.png"
  },
  "web_accessible_resources": ["stars.jpeg"]
}
