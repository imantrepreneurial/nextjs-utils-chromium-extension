{
  "description": "..... NextJS Utils Extension .......",
  "manifest_version": 2,
  "name": "NextJS Utilities Extension",
  "version": "1.0",
  "homepage_url": "https://github.com/",
  "icons": {
    "32": "icons/icon-N-32x32.png"
  },
  "permissions": ["activeTab", "contextMenus"],
  "background": {
    "scripts": ["background.js", "lib/jquery.min.js", "nextjs1.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "lib/jquery.min.js",
        "nextjs1.js"
      ]
    }
  ],
  "web_accessible_resources": ["*"],
  "browser_action": {
    "default_icon": "icons/icon-N-32x32.png"
  }
}
